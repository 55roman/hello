package ffroman.display{	import flash.display.Shape;	import flash.display.Sprite;	import flash.display.SimpleButton;	public dynamic class SimpleShapes {		public static function drawSquare($size:uint= 4,$col:uint = 0x333333):Shape{			var s = new Shape();			s.graphics.beginFill($col,1);			s.graphics.moveTo(-$size/2,-$size/2);			s.graphics.lineTo($size/2,-$size/2);			s.graphics.lineTo($size/2,$size/2);			s.graphics.lineTo(-$size/2,$size/2);			return s;		}	}}